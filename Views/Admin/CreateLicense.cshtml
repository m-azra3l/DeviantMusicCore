@model DeviantMusicCore.Models.ProductLicense

@{
    ViewData["Title"] = "Create License";
    Layout = "_AdminLayout";
}

<h3>Add License</h3>
<form method="post" action="CreateLicense">
    <div class="form-group">        
        <div class="col-12">
            <label asp-for="License"></label>
            <br/>
            <input asp-for="License" class="form-control" />
            <br/>
        </div>        
        <div class="form-group">
            <input type="submit" class="btn btn-success" value="Create" />
            <a asp-action="LicenseList" class="btn btn-primary">Back</a>
        </div>

    </div>

</form>
