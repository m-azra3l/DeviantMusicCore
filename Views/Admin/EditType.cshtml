@model DeviantMusicCore.Models.SoftwareType

@{
    ViewData["Title"] = "Edit Type";
    Layout = "_AdminLayout";
}
<h3>Edit Type</h3>
<form method="post" action="EditType">
    <div class="form-group">        
        <div class="col-12">
            <label asp-for="Type"></label>
            <input asp-for="Type" class="form-control" />
            <br/>
        </div>        
        <div class="form-group">
            <input type="submit" class="btn btn-success" value="Update" />
            <a asp-action="TypeList" class="btn btn-primary">Back</a>
        </div>
    </div>
</form>
