@model DeviantMusicCore.Models.ExtrasLicense

@{
    ViewData["Title"] = "Edit Extras License";
    Layout = "_AdminLayout";
}
<h3>Edit License</h3>
<form method="post" action="EditExtras">
    <div class="form-group">        
        <div class="col-12">
            <label asp-for="License"></label>
            <input asp-for="License" class="form-control" />
            <br/>
        </div>        
        <div class="form-group">
            <input type="submit" class="btn btn-success" value="Update" />
            <a asp-action="ExtrasList" class="btn btn-primary">Back</a>
        </div>
    </div>
</form>
